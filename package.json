    "electron-canvas-to-buffer": "2.0.0",
    "electron-debug": "^1.1.0",
    "electron-window-state": "^4.1.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "font-awesome": "^4.7.0",
    "get-folder-size": "^1.0.0",
    "immutable": "^3.8.1",
    "is-ipfs": "0.4.8",
    "material-ui": "1.0.0-beta.35",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "rand-token": "^0.4.0",
    "react": "15.6.2",
    "react-autosuggest": "^9.3.1",
    "react-avatar-editor": "9.1.1",
    "react-dom": "15.6.1",
    "react-feather": "^1.0.8",
    "react-fontawesome": "^1.5.0",
    "react-hot-loader": "3.0.0-beta.6",
    "react-moment": "0.2.4",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-action-buffer": "1.1.0",
    "redux-actions": "^2.0.1",
    "redux-form": "6.8.0",
    "redux-persist": "^4.8.0",
    "redux-persist-immutable": "^4.2.0",
    "redux-persist-node-storage": "^1.0.2",
    "redux-persist-transform-immutable": "^4.2.0",
    "redux-thunk": "^2.2.0",
    "roboto-fontface": "^0.8.0",
    "source-map-support": "^0.4.10",
    "throttle-debounce": "^1.0.1"
  },
  "devEngines": {
    "node": ">=7.x",
    "npm": ">=4.x"
  }
}
