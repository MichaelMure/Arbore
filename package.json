{
  "name": "arbore",
  "productName": "Arbore",
  "version": "0.0.0",
  "description": "Friend-to-friend filesharing app to save the world from dystopia",
  "main": "main.js",
  "scripts": {
    "lint": "eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "lint-fix": "npm run lint -- --fix",
    "hot-server": "cross-env NODE_ENV=development node -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config ./webpack.config.development.js",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "build": "npm run build-main && npm run build-renderer",
    "start": "cross-env NODE_ENV=production electron ./app/",
    "flow": "flow",
    "flow-typed": "flow-typed install",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./app/main.development",
    "postinstall": "concurrently \"install-app-deps\" \"node node_modules/fbjs-scripts/node/check-dev-engines.js package.json\"",
    "dev": "cross-env START_HOT=1 npm run hot-server",
    "package": "npm run build && build --publish never",
    "package-win": "npm run build && build --win --x64",
    "package-linux": "npm run build && build --linux",
    "package-all": "npm run build && build -mwl",
    "install": "npm-git install"
  },
  "browserslist": "electron 1.4",
  "build": {
    "productName": "Arbore",
    "appId": "org.michaelmure.arbore",
    "category": "",
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "files": [
      "dist/",
      "node_modules/",
      "app.html",
      "main.js",
      "main.js.map",
      "package.json"
    ],
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ]
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichaelMure/TotallyNotArbore.git"
  },
  "author": {
    "name": "Michael MurÃ©",
    "email": "batolettre@gmail.com",
    "url": "https://github.com/MichaelMure"
  },
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/MichaelMure/TotallyNotArbore/issues"
  },
  "keywords": [
    "P2P",
    "F2F",
    "file sharing",
    "file transfer",
    "social",
    "ipfs"
  ],
  "homepage": "http://arbo.re",
  "devDependencies": {
    "asar": "^0.13.0",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-flow-runtime": "^0.8.0",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-es2015-classes": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-react": "^6.22.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "babili-webpack-plugin": "^0.0.11",
    "concurrently": "^3.1.0",
    "cross-env": "^3.1.4",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.26.1",
    "devtron": "^1.4.0",
    "electron": "^1.4.15",
    "electron-builder": "^13.3.2",
    "electron-devtools-installer": "^2.0.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.14.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-compat": "^1.0.1",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-flowtype-errors": "^3.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^19.0.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.9.0",
    "express": "^4.14.0",
    "fbjs-scripts": "^0.7.1",
    "file-loader": "^0.10.0",
    "flow-bin": "^0.38.0",
    "flow-runtime": "^0.8.0",
    "flow-typed": "^2.0.0",
    "html-webpack-plugin": "^2.26.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^18.1.0",
    "jsdom": "^9.9.1",
    "minimist": "^1.2.0",
    "npm-git-install": "^0.3.0",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "^15.4.2",
    "redux-logger": "^2.7.4",
    "sinon": "^1.17.7",
    "spectron": "^3.4.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-merge": "^2.4.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "electron-canvas-to-buffer": "^2.0.0",
    "electron-debug": "^1.1.0",
    "extract-text-webpack-plugin": "^2.0.0",
    "font-awesome": "^4.7.0",
    "immutable": "^3.8.1",
    "material-ui": "1.0.0-alpha.12",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "multihashes": "^0.4.5",
    "react": "^15.4.2",
    "react-avatar-editor": "^8.0.1",
    "react-dom": "^15.4.2",
    "react-fontawesome": "^1.5.0",
    "react-hot-loader": "3.0.0-beta.6",
    "react-moment": "^0.2.2",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.1",
    "redux-form": "^6.6.3",
    "redux-persist-immutable": "^4.2.0",
    "redux-persist-transform-immutable": "^4.2.0",
    "redux-thunk": "^2.2.0",
    "roboto-fontface": "^0.7.0",
    "source-map-support": "^0.4.10"
  },
  "gitDependencies": {
    "redux-persist": "https://github.com/MichaelMure/redux-persist.git"
  },
  "devEngines": {
    "node": ">=6.x",
    "npm": ">=3.x"
  }
}
