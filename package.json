    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^7.1.0",
    "express": "^4.14.0",
    "fbjs-scripts": "^0.8.0",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.52.0",
    "flow-runtime": "^0.14.0",
    "flow-typed": "^2.0.0",
    "html-webpack-plugin": "^2.26.0",
